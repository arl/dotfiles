/* Sort all entries:
 :4,$!jq 'to_entries | sort_by(.key) | from_entries'
*/
{
  "C_Cpp.intelliSenseEngine": "default",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[go.mod]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[go]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[rust]": {
    "editor.guides.bracketPairs": true
  },
  "editor.fontFamily": "  'L- Consolas', 'Consolas', 'Courier New', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": false,
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 100,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.mouseWheelZoom": true,
  "editor.renderControlCharacters": false,
  "editor.renderWhitespace": "none",
  "editor.roundedSelection": false,
  "editor.selectionHighlight": true,
  "editor.suggestSelection": "first",
  "editor.tokenColorCustomizations": {
    "[Minimal]": {
      "comments": "#c3d3e298"
    }
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.ignoreRecommendations": false,
  "extensions.supportUntrustedWorkspaces": {},
  "files.exclude": {
    "**/.*.swp": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.venv": true,
    "**/*.o": true,
    "**/node_modules": true,
    "**/tags": true,
    "target/debug": true,
    "target/release": true
  },
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "jsonc": false,
    "go": true,
    "shellscript": true
  },
  "githubPullRequests.pullBranch": "never",
  "go.delveConfig": {
    "dlvLoadConfig": {
      "followPointers": true,
      "maxVariableRecurse": 1,
      "maxStringLen": 64,
      "maxArrayValues": 64,
      "maxStructFields": -1
    },
    "apiVersion": 2,
    "showGlobalVariables": true
  },
  "go.formatTool": "gofumpt",
  "go.languageServerExperimentalFeatures": {
    "diagnostics": true
  },
  "go.testFlags": ["-v"],
  "go.useLanguageServer": true,
  "gopls": {
    "usePlaceholders": true,
    "staticcheck": true,
    "codelenses": {
      "generate": false,
      "gc_details": true
    }
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "peacock.favoriteColors": [
    {
      "name": "Angular Red",
      "value": "#b52e31"
    },
    {
      "name": "Auth0 Orange",
      "value": "#eb5424"
    },
    {
      "name": "Azure Blue",
      "value": "#007fff"
    },
    {
      "name": "C# Purple",
      "value": "#68217A"
    },
    {
      "name": "Gatsby Purple",
      "value": "#639"
    },
    {
      "name": "Go Cyan",
      "value": "#5dc9e2"
    },
    {
      "name": "Java Blue-Gray",
      "value": "#557c9b"
    },
    {
      "name": "JavaScript Yellow",
      "value": "#f9e64f"
    },
    {
      "name": "Mandalorian Blue",
      "value": "#1857a4"
    },
    {
      "name": "Node Green",
      "value": "#215732"
    },
    {
      "name": "React Blue",
      "value": "#00b3e6"
    },
    {
      "name": "Something Different",
      "value": "#832561"
    },
    {
      "name": "Vue Green",
      "value": "#42b883"
    }
  ],
  "python.languageServer": "Pylance",
  "redhat.telemetry.enabled": false,
  "search.exclude": {
    "work/": true
  },
  "shellcheck.disableVersionCheck": true,
  "shellcheck.exclude": ["SC1091"],
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "/usr/bin/bash"
    },
    "zsh": {
      "path": "zsh"
    },
    "fish": {
      "path": "fish"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    },
    "pwsh": {
      "path": "pwsh",
      "icon": "terminal-powershell"
    }
  },
  "terminal.integrated.tabs.enabled": true,
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.colorCustomizations": {
    "minimap.background": "#00002299",
    "minimapSlider.background": "#595a9970",
    "minimapSlider.activeBackground": "#595a99b0",
    "minimapSlider.hoverBackground": "#595a9990"
  },
  "workbench.colorTheme": "Minimal",
  "workbench.iconTheme": "vscode-icons",
  "[go][go.mod]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "editor.occurrencesHighlight": "multiFile",
  "githubPullRequests.fileListLayout": "flat",
  "[st]": {
    "editor.formatOnSave": false
  },
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "[css]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "window.zoomLevel": 0,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.blame.editorDecoration.enabled": false,
  "docker.extension.enableComposeLanguageServer": false,
  "git.blame.statusBarItem.enabled": true
}
